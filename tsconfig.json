      "@tangential/core": [
        "src/lib/core"
      ],
      "@tangential/components": [
        "src/lib/view/components"
      ],
      "@tangential/input-widgets": [
        "src/lib/view/input-widgets"
      ],
      "@tangential/configurable-widgets": [
        "src/lib/view/configurable-widgets"
      ],
      "@tangential/admin-console": [
        "src/lib/view/modules/admin-console"
