{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "",
    "lib": [
      "es2016", "dom"
    ],
    "paths": {
      "@tangential/firebase-util": [
        "src/lib/util/firebase"
