{
  "project": {
    "version": "1.0.0-beta.25.5",
    "name": "tangential"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "demo-app/favicon.ico",
        "demo-app/vendor/MaterialIcons-Regular.woff2",
        "demo-app/vendor/MaterialIcons-Regular.woff",
        "demo-app/vendor/MaterialIcons-Regular.ttf"
      ],
      "index": "demo-app/index.html",
      "main": "demo-app/main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "demo-app",
      "mobile": false,
      "styles": [
        "demo-app/theme.scss",
        "demo-app/angular-material.layouts.min.css",
        "demo-app/vendor/material-icons.css",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "../node_modules/primeng/resources/primeng.min.css"
      ],
      "scripts": [],

      "environmentSource": "demo-app/environments/environment.ts",
      "environments": {
        "dev": "demo-app/environments/environment.dev.ts",
        "prod": "demo-app/environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
